<?xml version="1.0" encoding="UTF-8"?>
<pages><!-- 多个页面 -->
	<page pageName="Register"><!-- 一个页面 -->
		<!-- 一个一个的元素定位信息 -->
		<locator name="手机号码输入框" by="id" value="mobilephone" delay="5"></locator>
		<locator name="密码输入框" by="id" value="password"></locator>
		<locator name="重复密码输入框" by="id" value="pwdconfirm"></locator>
		<locator name="验证码输入框" by="id" value="verifycode"></locator>
		<locator name="注册按钮" by="id" value="signup-button" clickable="true"  delay="5"></locator>
		<locator name="重置按钮" by="name" value="reset"></locator>
		<locator name="提示信息元素" by="className" value="tips" displayed="true" delay="5"></locator>
	</page>

	<page pageName="Login">
		<locator name="手机号输入框" by="id" value="mobilephone"></locator>
		<locator name="密码输入框" by="cssSelector" value="input[type='password']"></locator>
		<locator name="登录按钮" by="id" value="login"></locator>
		<locator name="重置按钮" by="xpath" value="//*[@id='loginForm']/div[4]/div/input[2]"></locator>
		<locator name="提示信息元素" by="className" value="tips" displayed="true" delay="5"></locator>
	</page>
</pages>